{
	"name": "jyarali/to-sitemap",
	"description": "sitemap for to",
	"homepage": "https://github.com/jyarali/to-sitemap",
	"keywords": ["to-sitemap", "laravel", "php", "sitemap", "generator", "xml", "html", "google-news"],
	"license": "MIT",
	"authors": [
		{
			"name": "Mehdi Kord",
			"email": "info@mehdikord.ir",
			"role": "Developer",
			"homepage": "https://mehdikord.ir"
		}
	],
	"support": {
		"issues": "https://github.com/jyarali/to-sitemap/issues",
		"source": "https://gitlab.com/Laravelium/Sitemap",
		"wiki": "https://github.com/jyarali/to-sitemap/wikis/home"
	},
	"require": {
		"php": ">=7.2",
		"illuminate/support": "~6.0",
		"illuminate/filesystem": "~6.0"
	},
	"require-dev": {
		"laravel/framework": "~6.0",
		"phpunit/phpunit": "^7.5 || ^8.0",
		"orchestra/testbench-core": "3.8.*"
	},
	"autoload": {
		"psr-0": {
			"Laravelium\\Sitemap": "src/"
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"Laravelium\\Sitemap\\SitemapServiceProvider"
			]
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
